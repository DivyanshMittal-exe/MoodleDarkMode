{
	"name": "Moodle Dark Mode",
	"version": "1.2",
	"description": "Moodle Dark",
	"content_scripts": [ {
		"matches": [
			"https://moodle.iitd.ac.in/*"
		],
		"run_at": "document_idle",
		"js": ["general.js"]


	},{
		"matches": [
			"https://moodle.iitd.ac.in/grade/*"
			
		],
		"run_at": "document_idle",
		"js": ["grade.js"]
	},{
		"matches": [
			"https://moodle.iitd.ac.in/login/index.php"
		],
		"run_at": "document_idle",
		"js": ["login.js"]


	},{
		"matches": [
			"https://moodle.iitd.ac.in/my/*"
		],
		"run_at": "document_idle",
		"js": ["home.js"]


	},{
		"matches": [
			"https://moodle.iitd.ac.in/course/*"
		],
		"run_at": "document_idle",
		"js": ["course.js"]


	},{
		"matches": [
			"https://moodle.iitd.ac.in/mod/*"
		],
		"run_at": "document_idle",
		"js": ["mod.js"]


	},{
		"matches": [
			"https://moodle.iitd.ac.in/user/view/*"
		],
		"run_at": "document_idle",
		"js": ["user_view.js"]


	},{
		"matches": [
			"https://moodle.iitd.ac.in/user/*"
		],
		"run_at": "document_idle",
		"js": ["user.js"]


	}],
	"icons": {
		"36": "icons/icon36.png",
		"48": "icons/icon48.png",
		"96": "icons/icon96.png",
		"192": "icons/icon192.png",
		"512": "icons/icon512.png"
	},
	"manifest_version": 2
}
